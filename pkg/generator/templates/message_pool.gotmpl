{{- define "message_pool" -}}
    {{ if is_generate "POOL" -}}
        {{- /*gotype: github.com/e-tape/litepb/proto.Message*/ -}}
        {{ if .GetMemPoolMessage -}}
            {{ template "pool_name_message" . }} = &{{ import "sync" }}.Pool{New: func() any { return {{ template "message_constructor" . }} }}
        {{ end -}}
    {{ end -}}
{{- end -}}