{{- define "message_interface_set" -}}
    {{ if is_generate "INTERFACE" -}}
        {{- /*gotype: github.com/e-tape/litepb/proto.Message*/ -}}
        {{- $msg := . -}}
        {{ template "type_name" kv "msg" . "i_set" true }} interface {
        {{ range .GetProperties -}}
            {{ with .GetField -}}
                Set{{ .GetName }}({{ template "type_name" kv
                "type" .GetType
                "i_get" true
                }}) {{ template "type_name" kv "msg" $msg "i" true }}
            {{ end -}}
            {{ with .GetOneof -}}
                Set{{ .GetName }}({{ template "oneof_type_name" kv "msg" $msg "oneof" . }}) {{ template "type_name" kv "msg" $msg "i" true }}
                {{ range .GetFields -}}
                    Set{{ .GetName }}({{ template "type_name" kv "type" .GetType "i_get" true }}) {{ template "type_name" kv "msg" $msg "i" true }}
                {{ end -}}
            {{ end -}}
        {{ end -}}
        }
    {{ end -}}
{{- end -}}