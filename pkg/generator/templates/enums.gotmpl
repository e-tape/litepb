{{- define "enums" -}}
    {{ if is_generate "ENUM" -}}
        {{- range . -}}
            {{- /*gotype: github.com/e-tape/litepb/pkg/plugin.Enum*/ -}}
            {{ range (lines .GetComments) -}}
                //{{ . }}
            {{- end }}
            type {{ .GetName }} int32

            {{  $enum := . -}}
            const (
            {{ range .GetValues -}}
                {{ range (lines .Comments) -}}
                    //{{ . }}
                {{ end -}}
                {{ $enum.GetValuesPrefix }}_{{ .GetName }} {{ $enum.GetName }} = {{ .GetNumber }}
            {{ end -}}
            )
        {{ end -}}
    {{ end -}}
{{- end -}}