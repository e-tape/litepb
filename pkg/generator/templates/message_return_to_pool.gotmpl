{{- define "message_return_to_pool" -}}
    {{- /*gotype: github.com/e-tape/litepb/pkg/plugin.Message*/ -}}
    func (a *{{ .GetName }}){{ template "func_name_message_return_to_pool" }}() {
    {{ if gt (len .GetProperties) 0 -}}
        if a == nil { return }
        a.Reset()
        {{ if .GetWithMemPool -}}
            {{ template "pool_name_message" . }}.Put(a)
        {{ end -}}
    {{ end -}}
    }
{{- end -}}